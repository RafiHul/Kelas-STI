<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:background="#E0E6E7">

    <TextView
        android:id="@+id/textViewPickMahasiswaName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/inter"
        android:text="Wahyudin"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewPickMahasiswaNim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="24121000"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintStart_toStartOf="@+id/textViewPickMahasiswaName"
        app:layout_constraintTop_toBottomOf="@+id/textViewPickMahasiswaName" />
</androidx.constraintlayout.widget.ConstraintLayout>